{
  "name": "monorepo",
  "version": "0.1.0",
  "private": true,
  "workspaces": {
    "packages": [
      "common",
      "server",
      "client"
    ],
    "nohoist": [
      "client/**",
      "server/**"
    ]
  },
  "engines": {
    "node": "18"
  },
  "packageManager": "yarn@3.2.3",
  "dependencies": {
    "prisma": "4.3.0"
  },
  "scripts": {
    "start:asker": "npm --prefix client/asker start",
    "start:debugger": "npm --prefix client/asker start",
    "start:dataloader": "npm --prefix server/dataloader start",
    "start:api": "npm --prefix server/api start"
  }
}
